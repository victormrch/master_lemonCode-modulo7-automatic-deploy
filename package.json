{
  "name": "master_lemoncode-modulo3",
  "version": "1.0.0",
  "description": "practice of module 3 of master degree dedicated to webpack",
  "main": "index.js",
  "scripts": {
    "start": "run-p -l type-check:watch start:dev",
    "start:dev": "webpack serve --config webpack.dev.js",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check --watch",
    "build:dev": "npm run type-check && webpack --config webpack.dev.js",
    "build:prod": "npm run type-check && webpack --config webpack.prod.js",
    "build:perf": "webpack --config webpack.perf.js",
    "deploy": "gh-pages -d dist"
  },
  "author": "Victor Manuel Ramon",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "css-loader": "^5.2.7",
    "gh-pages": "^3.2.3",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.2",
    "mini-css-extract-plugin": "^2.1.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.35.2",
    "sass-loader": "^12.1.0",
    "typescript": "^4.3.5",
    "webpack": "^5.44.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
